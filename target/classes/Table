CREATE DATABASE foodapp;
USE foodapp;

CREATE TABLE food(
	id BIGINT auto_increment primary key,
    name varchar(250) not null,
    description varchar(300) not null
);

INSERT INTO food (name, description) VALUES
('Pizza', 'Delicious cheese pizza'),
('Burger', 'Tasty and Crispy Burger'),
('Sushi', 'Fresh sushi with salmon'),
('FrenchFries', 'Peri Peri one are too spicy and crispy'),
('Salad', 'Fresh green salad')
;
INSERT INTO food (name, description)
VALUES ('Salad', 'Fresh green salad');

select * from food;
